<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    android:background="@color/white"
    tools:context=".PapersFragment">
    <RelativeLayout
        android:id="@+id/session"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true">

            <TextView
                android:id="@+id/txt_session"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="5dp"
                android:text="Session"
                android:textColor="#000"
                android:textSize="16sp"
                android:textStyle="bold" />
            <Button
                android:id="@+id/session_20_21"
                android:layout_width="170dp"
                android:layout_height="35dp"
                android:layout_below="@id/txt_session"
                android:layout_centerHorizontal="true"
                android:background="@drawable/button_default"
                android:text="2020-2021"
                android:singleLine="true"
                android:textAllCaps="false" />
        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_below="@id/session"
        android:id="@+id/board"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="4dp">

            <TextView
                android:id="@+id/txt_board"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginBottom="5dp"
                android:text="Board/University"
                android:layout_centerHorizontal="true"
                android:textColor="#000"
                android:textSize="16sp"
                android:textStyle="bold" />
            <RelativeLayout
                android:id="@+id/boards_linearlayout"
                android:layout_below="@id/txt_board"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true">
                <Button
                    android:id="@+id/sy_dp"
                    android:layout_width="170dp"
                    android:layout_height="35dp"
                    android:background="@drawable/button_default"
                    android:layout_toEndOf="@id/sy_ku"
                    android:text="HSBTE/Diploma"
                    android:textAllCaps="false"
                    android:singleLine="true"
                    android:layout_marginStart="10dp"
                    />
                <Button
                    android:id="@+id/sy_ku"
                    android:layout_width="170dp"
                    android:layout_height="35dp"
                    android:background="@drawable/button_default"
                    android:text="Kurukshetra University"
                    android:textAllCaps="false" />
                <Button
                    android:id="@+id/ymca"
                    android:layout_width="170dp"
                    android:layout_height="35dp"
                    android:layout_below="@id/sy_dp"
                    android:layout_marginTop="4dp"
                    android:layout_marginBottom="4dp"
                    android:background="@drawable/button_default"
                    android:text="J.C. Bose University"
                    android:textAllCaps="false"/>

                <Button
                    android:id="@+id/gju"
                    android:layout_width="170dp"
                    android:layout_height="35dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="4dp"
                    android:layout_marginBottom="4dp"
                    android:layout_below="@id/sy_ku"
                    android:layout_toEndOf="@id/ymca"
                    android:background="@drawable/button_default"
                    android:text="GJU Hisar"
                    android:textAllCaps="false" />
            </RelativeLayout>

        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
